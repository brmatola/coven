{
  "name": "@coven/client-ts",
  "version": "1.0.0",
  "description": "TypeScript client for Coven daemon API",
  "private": true,
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": "./dist/src/index.js",
    "./*": "./dist/src/*"
  },
  "scripts": {
    "generate": "bash scripts/generate-client.sh",
